<Window x:Class="ConrolWorkGruznykh.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ConrolWorkGruznykh.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Content="Авторизоваться" Command="{Binding OpenLoginCommand}" Width="200" Margin="10"/>
            <Button Content="Зарегаться" Command="{Binding OpenRegisterCommand}" Width="200" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
